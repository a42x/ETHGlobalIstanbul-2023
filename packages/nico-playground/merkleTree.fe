contract MerkleProof{
    pub fn verify(root: bytes32, leaf: bytes32, proof: Array<bytes32, 100>) -> bool{
        let computed_hash: bytes32 = leaf

        for element in proof{
            if computed_hash < element{
                computed_hash = keccak256(computed_hash + element)
            }
            else{
                computed_hash = keccak256(element + computed_hash)
            }
        }
        return computed_hash == root
    }
}
